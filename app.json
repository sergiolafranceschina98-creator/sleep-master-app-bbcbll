{
  "expo": {
    "name": "Sleep Master",
    "slug": "Sleep Master",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-uve.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app-icon-uve.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0E1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sleepmaster.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIRequiresFullScreen": false,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "UIViewControllerBasedStatusBarAppearance": true
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-uve.png",
        "backgroundColor": "#0F0E1A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sleepmaster.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "Sleep Master",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}